{
  "modules": [
    "modules.setup",
    "modules.python",
    "modules.virtualenv",
    "modules.waf",
    "modules.django"
  ],
  "setup_modules": [
    "setup_modules.python",
    "setup_modules.pybuildtool"
  ],

  "python_version": "3.9",
  "waf_version": "2.0.23",

  "default_project": "web",
  "default_variant": "development",

  "configuration": {
    "development": {
      "web": {
        "shell_env": {
          "CONFIG_FILENAMES": [
            "etc/development/logging.json",
            "etc/development/web/account.json",
            "etc/development/web/application.json",
            "etc/development/web/database.json",
            "etc/development/web/server.json"
          ]
        }
      }
    }
  }
}
